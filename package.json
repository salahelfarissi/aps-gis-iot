{
  "name": "cesium-gis-iot",
  "version": "0.0.1",
  "description": "A Cesium-IoT platform.",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "coverage": "jest --coverage",
    "buildDev": "del build && webpack --mode development --config webpack.server.config.js && webpack --mode development --config webpack.dev.config.js",
    "buildProd": "del build && webpack --mode production --config webpack.server.config.js && webpack --mode production --config webpack.prod.config.js",
    "start": "node ./build/server.js"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js"
    }
  },
  "dependencies": {
    "express": "^4.17.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "engines": {
    "node": "16.16.0"
  },
  "license": "MIT",
  "keywords": [
    "cesium",
    "webpack",
    "express"
  ],
  "devDependencies": {
    "@babel/core": "^7.19.0",
    "@babel/preset-env": "^7.19.0",
    "@babel/preset-react": "^7.18.6",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.4.2",
    "babel-loader": "^8.2.5",
    "babel-preset-react": "^6.24.1",
    "bootstrap": "^5.2.1",
    "cesium": "^1.97.0",
    "copy-webpack-plugin": "11.0.0",
    "css-loader": "^6.7.1",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^7.32.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-react": "^7.31.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "5.5.0",
    "mini-css-extract-plugin": "^2.6.1",
    "open": "^8.4.0",
    "os-browserify": "^0.3.0",
    "pg": "^8.8.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-refresh": "^0.14.0",
    "react-router-dom": "5.2.0",
    "reactstrap": "^9.1.4",
    "resium": "^1.15.1",
    "sass": "^1.54.9",
    "sass-loader": "^13.0.2",
